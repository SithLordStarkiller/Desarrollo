        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="Response" type="tns:ResponseWS">
              <xsd:annotation>
                <xsd:documentation>
                          Para Codigo y Descripcion de respuesta .
                          Ya descripto en el tipo complejo ResponseWS . </xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="Atencion" type="tns:Atencion">
              <xsd:annotation>
                <xsd:documentation>
                          Para datos de atencion de operacion .
                          Ya descripto en el tipo complejo Atencion . </xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Codigos" type="tns:ListaCodigoOperacionDescripcion">
              <xsd:annotation>
                <xsd:documentation>
                          Para devolver los Codigos de Operaciones .
                          Ya descripto en el tipo complejo ListaCodigoOperacionDescripcion .
                          -&gt; Si la respuesta es rechazo, no estara presente
                          -&gt; Se la respuesta es aprobación, estara presente y dentro de
                             este habra una lista de 1 o + Codigos de Operacion . </xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
