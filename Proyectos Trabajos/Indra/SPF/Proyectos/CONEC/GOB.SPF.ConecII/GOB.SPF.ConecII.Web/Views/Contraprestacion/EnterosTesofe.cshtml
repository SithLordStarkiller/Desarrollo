
<style type="text/css">
    .header-filter {
        line-height: 1em;
        font-size: 1.2em;
        font-weight: bold;
        display: block;
        color: #777;
        border-bottom: 1px solid #F2F2F2;
    }
</style>

<ol class="breadcrumb">
    <li><span class="glyphicon glyphicon-home" aria-hidden="true"></span></li>
    <li><a href="#">Contraprestasión</a></li>
    <li><a href="~/Contraprestacion/Cetes" class="active">Cetes</a></li>
</ol>
<div class="flip">
    <div class="front">
        <div class="top-buffer">
            <h3>@ViewBag.Title</h3>
            <hr class="red">
        </div>
        <div class="form-group">
            <div class="panel">
                <div class="panel-body">
                    <div class="col-sm-4">
                        <label class="control-label">Razón social:</label>
                        <input type="text" id="razonSocial" name="razonSocial" class="form-control" placeholder="Razón social" />
                    </div>
                    <div class="col-sm-3">
                        <label class="control-label">RFC:</label>
                        <input type="text" id="rfc" name="rfc" class="form-control" placeholder="RFC" />
                    </div>
                    <div class="col-sm-3">
                        <label class="control-label">Llave de pago:</label>
                        <input type="text" id="llavePago" name="llavePago" class="form-control" placeholder="Llave de pago" />
                    </div>
                    <div class="col-sm-2">
                        <label class="control-label">No. de operación:</label>
                        <input type="text" id="numeroOperacion" name="numeroOperacion" class="form-control" placeholder="Número de operación" />
                    </div>
                </div>
            </div>
        </div>
        <div class="form-group">
            <div class="panel col-sm-6">
                <div class="panel-body">
                    <span class="header-filter">Presentación</span>
                    <div class="col-sm-6">
                        <label class="control-label">Fecha inicial:</label>
                        <input type="text" id="fechaInicialPresentacion" name="fechaInicialPresentacion" class="form-control" placeholder="dd/mm/yyyy" />
                    </div>
                    <div class="col-sm-6">
                        <label class="control-label">Fecha final:</label>
                        <input type="text" id="fechaFinalPresentacion" name="fechaFinalPresentacion" class="form-control" placeholder="dd/mm/yyyy" />
                    </div>
                </div>
            </div>
        </div>
        <div class="form-group">
            <div class="panel col-sm-6">
                <div class="panel-body">
                    <span class="header-filter">Carga</span>
                    <div class="col-sm-6">
                        <label class="control-label">Fecha inicial:</label>
                        <input type="text" id="fechaInicialCarga" name="fechaInicialCarga" class="form-control" placeholder="dd/mm/yyyy" />
                    </div>
                    <div class="col-sm-6">
                        <label class="control-label">Fecha final:</label>
                        <input type="text" id="fechaFinalCarga" name="fechaFinalCarga" class="form-control" placeholder="dd/mm/yyyy" />
                    </div>
                </div>
            </div>
        </div>

        <div class="col-sm-4 col-sm-offset-9">
            <div class="form-group">
                <button type="button" class="btn btn-default" id="btnBuscar">
                    <span class="glyphicon glyphicon-search"></span>
                    Buscar
                </button>
                <button type="button" class="btn btn-default" id="btnCreate">
                    <span class="glyphicon glyphicon-upload"></span>
                    Cargar
                </button>
            </div>
        </div>

        <div id="divEnterosTesofe"></div>
    </div>
    <div class="back"></div>
</div>

@section Scripts{
    <script src="~/Scripts/jquery/jquery-ui.js"></script>
    <script src="~/Scripts/conec/contraprestacion/enterostesofe.js"></script>

    <link href="~/Content/jquery.fileupload.css" rel="stylesheet" />

    <script src="~/Scripts/Controls/jquery.fileupload-ui.js"></script>
    <script src="~/Scripts/Controls/jquery.fileupload.js"></script>
    <script src="~/Scripts/Controls/jquery.iframe-transport.js"></script>

}