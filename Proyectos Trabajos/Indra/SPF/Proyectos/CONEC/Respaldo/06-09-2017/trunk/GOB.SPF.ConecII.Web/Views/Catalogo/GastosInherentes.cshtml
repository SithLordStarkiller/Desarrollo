@{
    ViewBag.Title = "Consulta Gastos Inherentes";
}
@section Styles{
    <link href="~/Content/jquery-ui.css" rel="stylesheet" />
}
<ol class="breadcrumb">
    <li><span class="glyphicon glyphicon-home" aria-hidden="true"></span></li>
    <li><a href="#">Catalogos</a></li>
    <li><a href="~/Catalogo/GastosInherentes" class="active">Gastos Inherentes</a></li>
</ol>
<div class="flip">
    <div class="front">
        <div class="top-buffer">
            <h3>@ViewBag.Title</h3>
            <hr class="red">
        </div>
            <div class="col-sm-3 form-group">
                <label class="control-label">Estatus:</label>
                <div class="col-sm-12  form-group">
                    <label class="radio-inline" for="activo">
                        <input type="radio" name="estatus" value="true" checked="checked">Activo
                    </label>
                    <label class="radio-inline" for="inactivo">
                        <input type="radio" name="estatus" value="false">Inactivo
                    </label>
                </div>
            </div>
            <div class="ui-widget col-sm-3 form-group">
                <label class="control-label">Gastos Inherentes:</label>
                <input id="GastosInherentes" class="form-control dropdown-toggle" placeholder="Ingresa Nombre">
            </div>
            <div class="pull-right">
                <button type="button" class="btn btn-default" id="btnBuscar">
                    <span class="glyphicon glyphicon-search"></span>Buscar
                </button>
            </div>            
            <div id="divGastosInherente"></div>        
        </div>
    <div class="back">

    </div>
    <input type="hidden" id="IdGastosInherente" />
</div>
@section Scripts{ 
    <script src="~/Scripts/conec/catalogo/gastosInherente.js"></script>    
<script src="~/Scripts/jquery/jquery-ui.js"></script>

}

